/**app.wxss**/
@import 'style/weui.wxss';

.theme-color {
  color: #827f2e;
}
.theme-bg {
  background-color: #827f2e;
}
.theme-bg-light {
  background-color: #a8a546;
}

.fl {
  float: left;
}
.fr {
  float: right;
}
.pd-20 {
  padding: 20rpx 10rpx;
}
.md-20 {
  margin-bottom: 20rpx;
}
.clearfix::after {
  content: '.';
  display: block;
  clear: both;
  height: 0;
  visibility: hidden;
}
.container {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  /* padding: 200rpx 0; */
  box-sizing: border-box;
  color: #555;
  font-size: 28rpx;
} 
